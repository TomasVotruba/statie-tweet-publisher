{
    "name": "tomasvotruba/statie-tweet-publisher",
    "description": "Statie Plugin that automates tweet posting from Posts",
    "bin": [
        "bin/publish-new-tweet"
    ],
    "require": {
        "php": "^7.2",
        "symplify/statie": "^5.2",
        "symplify/package-builder": "^5.2",
        "nette/utils": "^2.5",
        "J7mbo/twitter-api-php": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "TomasVotruba\\StatieTweetPublisher\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TomasVotruba\\StatieTweetPublisher\\Tests\\": "tests"
        }
    }
}
